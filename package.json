{
  "name": "@quintype/framework",
  "version": "2.70.0-beta.1",
  "description": "Libraries to help build Quintype Node.js apps",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm install && npm test && git diff --quiet && git tag -m '' -a v$(cat package.json | grep version | head -n1  | cut -d \\\" -f 4) && git push --follow-tags origin",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --recursive --require ./test/babel",
    "coverage": "nyc --all npm test",
    "coverage-html": "nyc --all --reporter=html npm test",
    "sync-files-to": "npx onchange --verbose --wait --await-write-finish 'client/**/*' 'server/**/*' 'isomorphic/**/*' -- ./bin-dev-scripts/sync-to.sh "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quintype/quintype-node-framework.git"
  },
  "keywords": [
    "quintype"
  ],
  "author": "Quintype Developers <dev-core@quintype.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/quintype/quintype-node-framework/issues"
  },
  "homepage": "https://github.com/quintype/quintype-node-framework#readme",
  "dependencies": {
    "@quintype/components": "^1.33.0-beta.1",
    "array.prototype.flatmap": "^1.2.1",
    "atob": "^2.0.3",
    "cluster": "^0.7.7",
    "compression": "^1.7.1",
    "dot-prop-immutable": "^1.4.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "get-youtube-id": "^1.0.0",
    "http-proxy": "^1.16.2",
    "js-yaml": "^3.10.0",
    "morgan": "^1.9.0",
    "object.omit": "^3.0.0",
    "quintype-backend": "^1.12.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "sleep-promise": "^2.0.0",
    "url": "^0.11.0",
    "winston": "3.0.0-rc1"
  },
  "devDependencies": {
    "@quintype/build": "^1.2.0",
    "babel-plugin-quintype-assets": "^1.0.2",
    "babel-plugin-react-css-modules": "^3.3.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-tree-shaking": "^1.0.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "history": "^4.7.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "onchange": "^4.1.0",
    "supertest": "^3.0.0"
  },
  "peerDependencies": {
    "@quintype/seo": "^1.4.1"
  },
  "nyc": {
    "exclude": [
      "coverage",
      "test",
      "client/*.js"
    ],
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100
  }
}
